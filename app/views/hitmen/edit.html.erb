<h4 style="padding-top:10px;">Edit your profile</h4>

<%= simple_form_for [@hitman] do |f| %>
  <%= f.input :name %>
  <%= f.input :bio, as: :text %>
  <%= f.input :location %>
  <%= f.input :method %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Commit changes", class: "btn btn-dark" %>
<% end %>
<br>
