<h1>Your Requests</h1>
<ul class="list-group">
  <% @jobs.each do |j| %>
   <li class="list-group-item">
    <%= "Hitman: #{j.hitman.name} - Location: #{j.hitman.location} - Method: #{j.hitman.method}" %>
    <%= link_to "View profile", hitman_path(j.hitman), class:"btn btn-dark" %>
  </li>
  <% end %>
</ul>
